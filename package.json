{
  "homepage": "https://valentinhojbota.github.io/near-app",
  "name": "near-dapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_ENV=dev parcel src/index.html --open",
    "test": "REACT_APP_ENV=dev jest test.js --useStderr",
    "predeploy": "parcel build src/index.html --no-source-maps",
    "deploy": "gh-pages -d dist",
    "test:contract": "(cd contract && cargo test -- --nocapture)",
    "build:contract": "(cd contract && ./build.sh)",
    "deploy:contract": "yarn build:contract && (near deploy -f --wasmFile out/main.wasm --accountId valih.testnet || exit 0)"
  },
  "dependencies": {
    "formik": "^2.2.9",
    "gh-pages": "^3.2.3",
    "near-api-js": "^0.44.2",
    "parcel": "^2.0.0-beta.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^5.1.0",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "jest": "^27.4.7",
    "jest-jasmine2": "^27.4.6",
    "prettier": "2.5.1"
  }
}
